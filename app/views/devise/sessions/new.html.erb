  <div class="yessir">
      <%= simple_form_for(resource, as: resource_name, remote:true, url: session_path(resource_name)) do |f| %>
      <%= f.error_notification %>
      
      <div class="form-inputs">
       <%= f.input :username,
        required: true,
        autofocus: true,
        placeholder: "Username"
        %>
      
        <%= f.input :email,
        required: true,
        autofocus: true,
        input_html: { autocomplete: "email" },
        placeholder: "Email"
        %>
      
        <%= f.input :password,
        required: true,
        input_html: { autocomplete: "current-password" },
        placeholder: "Password" 
        %>
       
        <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
      </div>
      
      

      
       <div class="form-actions mt-2">
       <div class="newseperator">
        <%= f.button :submit, " Log in", class: "btn btn-primary btn-block mb-2", id:"btnform2" %> 
      </div>
        <%= render "devise/shared/forgot_pw", class: "newforgot" %> 
       </div>
      
      <div class="newairwaves">
      <div class="separator my-3">Don't have an account?</div>
      </div>
      <div class="mokko">
      <%= render "devise/shared/signup_btn", id:"btnform2" %>
    </div>
   
<% end %>
